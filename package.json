{
  "name": "id3-tag-editor",
  "productName": "ID3 Tag Editor",
  "version": "1.0.0",
  "description": "A tag editor for files containing ID3 tags",
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "electron dist/main.bundle.js",
    "dev": "node webpack.devServer.js",
    "e2e": "cross-env NODE_ENV=production npm-run-all --parallel build:main build:renderer && jest .*\\.e2e\\.ts",
    "test": "jest .*\\.test\\.ts",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:main build:renderer && electron-builder",
    "build:main": "webpack --config webpack.main.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "postinstall": "cross-env-shell npm_config_runtime=electron npm_config_target=$(node -p \"require('./package.json').devDependencies.electron\" | tr -d '') npm install taglib2"
  },
  "build": {
    "appId": "id3-tag-editor",
    "mac": {
      "category": "music"
    },
    "files": [
      "./dist/**/*"
    ]
  },
  "keywords": [],
  "author": "Michael Tugby",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/uuid": "3.4.3",
    "@types/webdriverio": "4.10.3",
    "@vue/test-utils": "1.0.0-beta.24",
    "ajv": "6.5.2",
    "autoprefixer": "9.1.1",
    "cache-loader": "1.2.2",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "devtron": "1.4.0",
    "electron": "9.4.0",
    "electron-builder": "20.28.1",
    "electron-connect": "0.6.3",
    "electron-devtools-installer": "2.2.4",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.8",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.5.0",
    "jest-serializer-vue": "2.0.2",
    "mini-css-extract-plugin": "0.4.1",
    "node-loader": "0.6.0",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "postcss-loader": "3.0.0",
    "pug-plain-loader": "1.0.0",
    "source-map-support": "0.5.8",
    "spectron": "3.8.0",
    "style-loader": "0.22.1",
    "stylus-loader": "3.0.2",
    "ts-jest": "23.1.3",
    "ts-loader": "4.4.2",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "vue-jest": "2.6.0",
    "vue-loader": "15.3.0",
    "vue-template-compiler": "2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "pug": "^3.0.2",
    "stylus": "0.54.5",
    "taglib2": "^2.9.6",
    "uuid": "3.3.2",
    "vue": "2.5.17",
    "vue-electron": "1.0.6",
    "vue-property-decorator": "7.0.0",
    "vue-router": "3.0.1",
    "vuetify": "1.1.12",
    "vuex": "3.0.1",
    "vuex-class": "0.3.1",
    "vuex-persist": "1.4.3"
  }
}
